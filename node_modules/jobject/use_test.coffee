Jobject = require '../jobject'

class MyClass extends Jobject

  constructor: ()->
    @property 'name'
    @property 'title'
    @property 'author'

myClass = new MyClass()
myClass.setName 'Jesse Earle'
myClass.setTitle 'This is a Title'
myClass.setAuthor 'Stephen King'

console.log myClass.name()   # Jesse Earle
console.log myClass.title()  # This is a Title
console.log myClass.author() # Stephen King