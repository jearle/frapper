#!/usr/bin/env coffee

frap = require '../frapper'
pkg = require './package.json'
version = pkg.version

# program = new frap.Program 
#   version: version
#   commands:
#     'model [attributes]': 
#       description: 'Generates a mongoose model.'
#       options: 
#         '-v, --verbose': 'Prints out a detailed '
#       action: (attributes)->

program = new frap.Program
  version: version
  commands:
    'model [attributes]': (attributes)->
      console.log attributes
      

program.exec()

